<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <ThingDef Name="AnimalMilkingBase" ParentName="ApparelMakeableBase" Abstract="True">
        <techLevel>Neolithic</techLevel>
        <recipeMaker>
            <unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
            <recipeUsers>
                <li>ElectricTailoringBench</li>
                <li>HandTailoringBench</li>
            </recipeUsers>
        </recipeMaker>
        <graphicData>
            <texPath>AMG</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <statBases>
            <StuffEffectMultiplierInsulation_Cold>0.20</StuffEffectMultiplierInsulation_Cold>
            <StuffEffectMultiplierInsulation_Heat>0.05</StuffEffectMultiplierInsulation_Heat>
            <EquipDelay>1.5</EquipDelay>
        </statBases>
        <stuffCategories>
            <li>Fabric</li>
            <li>Leathery</li>
        </stuffCategories>
        <thingCategories>
            <li>Animal_Apparel</li>
        </thingCategories>
        <generateAllowChance>0</generateAllowChance>
        <apparel>
            <careIfWornByCorpse>false</careIfWornByCorpse>
            <bodyPartGroups>
                <li>Torso</li>
                <li>Neck</li>
                <li>Legs</li>
            </bodyPartGroups>
            <wornGraphicPath>AnimalMilkingGear/amg</wornGraphicPath>
            <layers>
                <li>OnSkin</li>
            </layers>
            <defaultOutfitTags>
                <li>Anything</li>
                <li>Soldier</li>
                <li>Worker</li>
            </defaultOutfitTags>
            <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
        </apparel>
        <stuffProps>
            <allowColorGenerators>false</allowColorGenerators>
        </stuffProps>
        <tickerType>Normal</tickerType>
        <colorGenerator Class="ColorGenerator_StandardApparel" />
    </ThingDef>

