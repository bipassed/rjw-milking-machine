<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <TraitDef>
        <defName>LM_NaturalHucow</defName>
        <commonality>0.1</commonality>
        <degreeDatas>
            <li>
                <label>natural hucow</label>
                <description>{PAWN_nameDef} naturally lactates more than most.</description>
            </li>
        </degreeDatas>
    </TraitDef>

    <TraitDef>
        <defName>LM_HighTestosterone</defName>
        <commonality>0.1</commonality>
        <degreeDatas>
            <li>
                <label>high testosterone</label>
                <description>{PAWN_nameDef} has a higher amount of testosterone.</description>
            </li>
        </degreeDatas>
    </TraitDef>

    <TraitDef>
        <defName>LM_NaturalCow</defName>
        <commonality>0.025</commonality>
        <degreeDatas>
            <li>
                <label>natural cow</label>
                <description>{PAWN_nameDef} has a very high amount of testosterone and naturally lactates more than most, making them prime for milking.</description>
            </li>
        </degreeDatas>
        <conflictingTraits>
            <li>LM_HighTestosterone</li>
            <li>LM_NaturalHucow</li>
          </conflictingTraits>
    </TraitDef>

</Defs>